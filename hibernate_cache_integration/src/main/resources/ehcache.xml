<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ehcache>

<ehcache>

	<diskStore path="java.io.tmpdir" />

	<cache name="ehcache" 
		maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="600" 
		timeToLiveSeconds="3600" 
		overflowToDisk="true" />
	<cache name="ServiceType.ById" 
		maxElementsInMemory="10000"
		eternal="false" 
		timeToIdleSeconds="600" 
		timeToLiveSeconds="3600"
		overflowToDisk="true" />
